%h1= t("liquid_theme.edit")
= form_for(@liquid_theme, :url => object_url, :html => { :method => :put }) do |f|
	= render :partial => "form", :locals => { :f => f }
	= render :partial => "admin/shared/edit_resource_links"
- %w{liquid_templates liquid_snippets theme_assets}.each do |act|
	= render :partial => "/admin/#{act}/index"